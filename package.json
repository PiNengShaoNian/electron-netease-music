{
  "name": "netease-music",
  "version": "0.1.0",
  "private": true,
  "main": "main.js",
  "homepage": "./",
  "author": {
    "name": "quguiyou",
    "email": "quguiyou1076@qq.com"
  },
  "description": "网易云音乐",
  "build": {
    "appId": "NeteaseMusic",
    "productName": "NeteaseMusic",
    "copyright": "Copyright 2019 ${author}",
    "extends": null,
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "target": [
        "msi",
        "nsis"
      ],
      "icon": "assets/icon.ico",
      "artifactName": "${productName}-Web-Setup-${version}.${ext}",
      "publisherName": "quguiyou"
    },
    "nsis": {
      "allowToChangeInstallationDirectory": true,
      "oneClick": false,
      "perMachine": false
    },
    "electronDownload": {
      "mirror": "https://npm.taobao.org/mirrors/electron/"
    },
    "extraMetadata": {
      "main": "./build/main.js"
    }
  },
  "dependencies": {},
  "scripts": {
    "start": "react-scripts start",
    "react-build": "react-scripts build",
    "electron-build": "electron-builder",
    "build": "yarn react-build && yarn electron-build",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "predist": "npm run build && npm run buildMain",
    "prepack": "npm run react-build && npm run buildMain",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "buildMain": "webpack",
    "electron-dev": "concurrently \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && nodemon --exec electron .\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@better-scroll/core": "^2.0.0-beta.2",
    "@better-scroll/mouse-wheel": "^2.0.0-beta.2",
    "@better-scroll/scroll-bar": "^2.0.0-beta.2",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "concurrently": "^5.0.1",
    "cross-env": "^6.0.3",
    "electron": "^15.5.5",
    "electron-builder": "^21.2.0",
    "electron-is-dev": "^1.1.0",
    "element-react": "^1.4.34",
    "element-theme-default": "^1.4.13",
    "good-storage": "^1.1.0",
    "lodash-es": "^4.17.15",
    "node-sass": "^4.13.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-devtools-electron": "^4.2.1",
    "react-dom": "^16.12.0",
    "react-hot-loader": "^4.12.18",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "react-transition-group": "^4.3.0",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^8.0.0",
    "wait-on": "^3.3.0",
    "webpack-cli": "^3.3.10",
    "xgplayer": "^2.3.8"
  }
}
